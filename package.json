  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/frostburn"
  },
  "files": [
    "dist"
  ],
  "types": "dist/src/index.d.ts",
  "devDependencies": {
    "@types/node": "^20.17.9",
    "gts": "^5.3.1",
    "typedoc": "^0.25.13",
